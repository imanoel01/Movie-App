@model List<Vidly.Models.Movie>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Movies List</h2>

@if (Model.Count<0)
{
    <h2> No Movies Currently</h2>
}

else
{

    foreach (var movie in Model)
    {

<table>
    <tr>
        <td> @Html.ActionLink(movie.Title,"Detail","Movies",new {id=movie.Id },null)   </td>
    </tr>
</table>
    }
}